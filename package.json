{
  "type": "module",
  "name": "el-buen-sabor",
  "version": "1.0.0",
  "description": "Backend para el sistema del buen sabor",
  "main": "index.js",
  "engines": {
    "node": "14.15.1"
  },
  "scripts": {
    "frontend": "cd frontend && npm start",
    "api": "nodemon index.js -e js",
    "dev": "concurrently --kill-others-on-fail \"npm run api\" \"npm run frontend\"",
    "start": "set DEBUG=express:* & nodemon index.js -e js && cd frontend && npm install && ng serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "cd frontend && npm install && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "sequelize": "^6.4.0"
  }
}
